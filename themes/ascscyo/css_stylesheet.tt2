/* stylesheet for ascscyo.org */
/* $Id: css_stylesheet.tt2 428 2006-08-30 02:00:57Z corrupt $ */

A {
    font-family: helvetica, verdana, sans-serif;
    color: #000000;
    text-decoration: none;
    border: none;
}

.ul_link {
    font-size: 12px;
    color: #000000;
    text-decoration: underline;
}

.normal_text {
    font-size: 12px;
    color: #000000;
}

.highlighted_text {
    font-size: 12px;
    color: #000000;
    background-color: #FFFF00;
}

.header_text {
    font-size: 16px;
    color: #000000;
}

.huge_text_blue {
    font-size: 34px;
    color: #012778;
}

.title_text {
    font-size: 24px;
    color: #000000;
}

.css_line {
    border-top: #000000 1px solid;
}

td.tiny_line {
    background-color: #000000;
}

.tiny_text {
    font-size: 10px;
}

.error_text {
    font-size: 10px;
    color: red;
}

.logo_place {
    position: absolute;
    margin-left: 0px; 
    height: 170px;
    width: 384px;
}

.title_place {
    position: absolute;
    margin-left: 420px;
    margin-top: 55px;
    height: 170px;
}

.watermark_place {
    position: absolute;
    margin-left: 220px;
    margin-top: 100px;
}

.cap {
    float: left;
    width: 42px;
    font-size: 62px;
    line-height: 60px;
    font-weight: normal;
    font-family: georgia;
}

.body_main {
    position: absolute;
    margin-top: 95px;
    margin-left: 0px;
}

.left_column {
    position: absolute;
    margin-top: 95px;
    margin-left: 0px;
}

.solo_article {
    position: absolute;
    text-align: justify;
    margin-left: 150px;
    margin-top: 95px;
    width: 600px;
}

.center_column {
    position: absolute;
    margin-top: 95px;
    margin-left: 220px;
}

.right_column {
    position: absolute;
    margin-top: 95px;
    margin-left: 658px;
}

.centerbox {
    background: transparent;
    text-align: justify;
    border: none;
    padding: 1px 1px 1px 1px;
    width: 420px;
    margin-left: 5px;
}

.centerbox_divider {
    background: transparent;
    width: 200px;
}

.box_spacer {
    border-bottom: 1px black solid;
}

.box_title {
    background: #EAEDF4;
    border: #012778 1px solid;
    text-align: center;
    color: #333333;
    border-bottom: none;
    font-size: 14px;
    width: 200px;
}

.box_link {
    padding: 1px;
    font-size: 12px;
    background: transparent;
}

.box_frame {
    margin-left: 5px;
    width: 202px;
}

.box_box {
    border: #444444 1px solid;
    width: 200px;
}

.s1 {
    border-right: #ccc 2px solid;
    border-bottom: #ccc 2px solid;
    width: 202px;
    left: 2px;
    top: 2px;
}

.overlay {
    position: absolute;
    border-top: #012778 2px solid;
    margin-left: 0px;
    margin-top: 85px;
    width: 100%;
}

.address_place {
    position: absolute;
    margin-left: 500px;
    margin-top: 74px;
    font-size: 12px;
    font-weight: bold;
    font-family: helvetica, arial, sans-serif;
}

table.no_border {
    border: none;
    background-color: #FFFFFF;
}

INPUT {
    color: #000000;
    border: #000000 1px solid;
}

TEXTAREA {
    color: #000000;
    border: #000000 1px solid;
    font-size: 12px;
    font-family: helvetica, verdana, sans-serif;
}

SELECT {
    color: #000000;
    border: #000000 1px solid;
    font-size: 12px;
    font-family: helvetica, verdana, sans-serif;
}

.text_input {
    color: #000000;
    border: #000000 1px solid;
    font-size: 12px;
    font-family: helvetica, verdana, sans-serif;
    font-weight: normal;
}

td.menu_area {
    background-color: #D0D7E3;
}

td.paltrow_0 {
    background-color: transparent;
    font-size: 12px;
    color: #000000;
}

td.paltrow_1 {
    background-color: #EAEDF4;
    font-size: 12px;
    color: #000000;
}

td.altrow_0 {
    background-color: #CCCCCC;
    font-size: 10px;
    color: #000000;
}

td.altrow_1 {
    background-color: #EEEEEE;
    font-size: 10px;
    color: #000000;
}

td.box {
    border: #4F5E7B 1px solid;
    background-color: #FFFFFF;
    color: #000000;
    font-family: helvetica, verdana, sans-serif;
}

td.box_tab {
    border: #4F5E7B 1px solid;
    background-color: #4F5E7B;
    color: #FFFFFF;
    font-family: helvetica, verdana, sans-serif;
    font-size: 12px;
    text-decoration: bold;
}

BODY {
    position: relative;
    font-family: helvetica, verdana, sans-serif;
    font-size: 12px;
    color: #000000;
    text-decoration: none;
    vertical-align: middle;
    margin-top: 2px;
    margin-bottom: 2px;
    margin-left: 2px;
    margin-right: 2px;
}

     <div id="container">
  <div id="topcontent"></div>
  <div id="singlecontent">
   <div class="clearer"> </div>
   <div class="singlepost">
       <h2 class="searchresult">Category - ([% fe.param('article_category') %])</h2>
          <div class="searchdetails">
        Archive for category [% fe.param('article_category') %]
     </div>
     [% FOREACH article = fe.ai.articles_by_category(fe.param('article_category'), 'create', display_articles, skip_articles) %]
           <h2 class="searchresult">
               <a href="/article/archived/[% article.id %]/" rel="bookmark" title="Permanent Link to [% article.subject %]">
               [% article.subject %]</a>
       </h2>
       <div class="searchinfo">
           ( <a href="/category/[% article.category %]/" title="View all posts in [% article.category %]" rel="category tag">[% article.category %]</a> )       
           [ [% FOREACH tag = fe.ti.tags('article', article.id) %]
               <a href="/tagged/[% tag.name %]/" title="View all posts tagged with [% tag.name %]">[% tag.name %]</a>
               [% UNLESS tag.is_last %]
                  /
               [% END %]
             [% END %]
           ]
       </div>

       <div class="clearer"> </div>
       <p>[% fe.ai.truncate(article, 'body', '512', 1) %]
</p>
       <div class="searchinfo">
        <a href="/article/archived/[% article.id %]/" rel="bookmark" title="Permanent Link to [% article.subject %]">
         (to view associated entry please click here)</a>

       </div>
    [% END %]
  </div> <!-- This closes the singlepost div-->
   <div class="postnavigation">
    <div class="right">
        [% IF fe.ai.previous_posts_by_category(display_articles, skip_articles, fe.param('article_category')) %]
            <a href="/category/[% fe.param('article_category') %]/[% paged + 1 %]/">previous posts + &#187;</a>
        [% END %]
    </div>
    <div class="left">
        [% IF paged > 1 %]
            <a href="/category/[% fe.param('article_category') %]/[% paged - 1 %]/">&#171; + newer posts</a>
        [% END %]        
    </div>
   </div>
   <div id="bottomcontent"> </div>
  </div> <!-- This closes the content div-->
 </div> <!-- This closes the container div-->
